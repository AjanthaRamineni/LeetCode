//O(n) time and O(1) space
class Solution {
    public int findNthDigit(int n) {
        int count = 1;
        long cur = 9;
        int result = 1;
        while(n>cur*count){
            n-=(cur*count);
            result = result*10;
            cur = (cur*10); 
            count++;
        }
        System.out.println(result);
        System.out.println(n);
        result+=(n-1)/count;
        String s = Integer.toString(result);
		return Character.getNumericValue(s.charAt((n - 1) % count));
    }
}
