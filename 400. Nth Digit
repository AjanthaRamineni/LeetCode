//O(1) time and space
class Solution {
    public int findNthDigit(int n) {
        if(n<1) return -1;
        int length = 1, start = 1;
        long count = 9;
        while(n>length*count){
            n-=length*count;
            length++;
            count*=10;
            start*=10;
        }
        start+=((n-1)/length);
        String s = String.valueOf(start);
        return Character.getNumericValue(s.charAt((n-1)%length));
    }
}
