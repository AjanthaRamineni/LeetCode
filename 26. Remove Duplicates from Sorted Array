//O(n) and O(1)
class Solution {
    public int removeDuplicates(int[] nums) {
        int index = 0;
        for(int n:nums){
            if(index<1 || n>nums[index-1]){
                nums[index++] = n;
            }
        }
        return index;
    }
}

//O(n) and O(n)
class Solution {
    public int removeDuplicates(int[] nums) {
        //Sanity Check
        if(nums==null || nums.length==0) return 0;
        int[] aux = new int[nums.length];
        int np = 0, ap = 0;
        aux[ap] = nums[np++];
        while(np<nums.length){
            if(nums[np]==aux[ap]){
                np++;
            }
            else{
                aux[++ap] = nums[np++];
            }
        }
        ap++;
        //Copy all the first ap elements from aux to nums
        for(int i=0;i<ap;i++){
            nums[i] = aux[i];
        }
        return ap;
    }
}
