//O(n) time and space
class Solution {
    public boolean isIsomorphic(String s, String t) {
        Map<Character, Character> map = new HashMap();
        if(s.length()!=t.length()) return false;
        for(int i=0;i<s.length();i++){
            char a = s.charAt(i);
            char b = t.charAt(i);
            if(!map.containsKey(a)){
                if(map.containsValue(b)) return false;
                map.put(a,b);
            }
            else{
                char c = map.get(a);
                if(c!=b) return false;
            }
        }
        return true;
    }
}
