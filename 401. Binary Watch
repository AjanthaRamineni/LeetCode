//O(1) time and space
class Solution {
    public List<String> readBinaryWatch(int num) {
        List<String> list = new ArrayList();
        for(int h=0;h<12;h++){
            for(int m=0;m<60;m++){
                if(Integer.bitCount(m)+Integer.bitCount(h)==num)
                    list.add(String.format("%d:%02d",h,m));
            }
        }
        return list;
    }
}
