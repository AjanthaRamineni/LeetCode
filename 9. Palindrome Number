//O(n) time and O(1) space
class Solution {
    public boolean isPalindrome(int x) {
        if(x<0) return false;
        int y = x;
        long result = 0;
        while(y!=0){
            result = result*10 + y%10;
            y/=10;
        }
        if(result<Integer.MIN_VALUE || result>Integer.MAX_VALUE) return false;
        return ((int)result == x);
    }
}
