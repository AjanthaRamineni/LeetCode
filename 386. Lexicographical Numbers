//O(n) time and space
class Solution {
    public List<Integer> lexicalOrder(int n) {
        List<Integer> list = new ArrayList();
        for(int i=1;i<10;i++)
            dfs(list, i, n);
        return list;
    }
    public void dfs(List<Integer> list, int cur, int n){
        if(cur>n)
            return;
        else{
            list.add(cur);
            for(int i=0;i<10;i++){
                if(cur*10+i>n)
                    return;
                dfs(list, cur*10+i, n);
            }
        }
    }
}
